<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="rectangle">
            <gradient
                android:type="linear"
                android:startColor="#FF6D69FF"
                android:endColor="#FF00E5FF"
                android:angle="135" />
        </shape>
    </item>
    <item android:gravity="center">
        <bitmap
            android:gravity="center"
            android:src="@drawable/mindkite_splash_foreground" />
    </item>
</layer-list>
